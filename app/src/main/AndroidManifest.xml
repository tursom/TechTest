<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="cn.tursom.techtest">
	
	<uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
	<uses-permission android:name="android.permission.INTERNET"/>
	
	<application
			android:allowBackup="true"
			android:icon="@mipmap/ic_launcher"
			android:label="@string/app_name"
			android:roundIcon="@mipmap/ic_launcher_round"
			android:supportsRtl="true"
			android:theme="@style/AppTheme"
			tools:ignore="GoogleAppIndexingWarning">
		<activity
				android:name=".ClassScheduleControllerActivity"
				android:label="@string/title_activity_class_schedule_controller"
				android:theme="@style/AppTheme.NoActionBar">
		</activity>
		<activity
				android:name=".QRCodeTestActivity"
				android:label="@string/title_activity_qrcode_test"
				android:theme="@style/AppTheme.NoActionBar">
		</activity>
		<activity
				android:name=".EncryptionActivity"
				android:label="@string/title_activity_encryption"
				android:theme="@style/AppTheme.NoActionBar">
		</activity>
		<activity android:name=".NetworkTestActivity">
		</activity>
		<activity android:name=".SensorActivity">
		</activity>
		<activity android:name=".MainActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>
				
				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
	</application>

</manifest>